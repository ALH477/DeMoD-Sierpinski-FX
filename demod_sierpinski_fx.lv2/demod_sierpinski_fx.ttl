@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
<https://faustlv2.bitbucket.io/demod_sierpinski_fx>
       a lv2:Plugin ;
       doap:name "DeMoD Sierpinski FX" ;
       lv2:binary <demod_sierpinski_fx.so> ;
       lv2:requiredFeature urid:map ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRTCapable ;
       doap:maintainer [ foaf:name "DeMoD + Grok" ] ;
       rdfs:comment "Argent Metal Edition â€” 96 BPM industrial metal fractal effect" ;
       doap:revision "2.4" ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "Bypass_0" ;
	lv2:name "Bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
	lv2:default 0.00000 ;
	lv2:minimum 0.00000 ;
	lv2:maximum 1.00000 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "Intensity_1" ;
	lv2:name "Intensity" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 100 ;
	lv2:default 0.85 ;
	lv2:minimum 0 ;
	lv2:maximum 1 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "Feedback_2" ;
	lv2:name "Feedback" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 98 ;
	lv2:default 0.52 ;
	lv2:minimum 0 ;
	lv2:maximum 0.98 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "BPM_3" ;
	lv2:name "BPM" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 120 ;
	lv2:default 96 ;
	lv2:minimum 60 ;
	lv2:maximum 180 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "Master_Gain_4" ;
	lv2:name "Master Gain" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 240 ;
	lv2:default 3 ;
	lv2:minimum -12 ;
	lv2:maximum 12 ;
	units:unit [
            a            units:Unit ;
            units:name   "dB" ;
            units:symbol "dB" ;
            units:render "%f dB"
	] ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 5 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 6 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ]
.
